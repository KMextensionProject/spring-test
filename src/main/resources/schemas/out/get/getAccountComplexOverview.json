{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"default_account": {
			"type": "object",
			"properties": {
				"account_id": {
					"type": "string"
				},
				"currency": {
					"type": "string"
				},
				"currency_acronym": {
					"type": "string"
				},
				"balance": {
					"type": "number"
				},
				"best_buy_order_rate": {
					"type": "number"
				}
			},
			"required": [
				"account_id",
				"currency",
				"currency_acronym",
				"balance",
				"best_buy_order_rate"
			]
		},
		"trading_account": {
			"type": "object",
			"properties": {
				"account_id": {
					"type": "string"
				},
				"currency": {
					"type": "string"
				},
				"currency_acronym": {
					"type": "string"
				},
				"balance": {
					"type": "number"
				}
			},
			"required": [
				"account_id",
				"currency",
				"currency_acronym",
				"balance"
			]
		}
	},
	"required": [
		"default_account",
		"trading_account"
	]
